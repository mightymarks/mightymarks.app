{
	"hosting": {
		"public": "public",
		"ignore": ["firebase.json", "**/.*", "**/node_modules/**"],
		"headers": [
			{
				"source": "**/*.@(woff|woff2)",
				"headers": [
					{
						"key": "Access-Control-Allow-Origin",
						"value": "*"
					}
				]
			},
			{
				"source": "**/*.@(js|css|woff|woff2|jpg|jpeg|gif|svg|png)",
				"headers": [
					{
						"key": "Cache-Control",
						"value": "public, max-age=315360000, immutable"
					}
				]
			},
			{
				"source": "/",
				"headers": [
					{
						"key": "Link",
						"value": "</static/roboto-mono-latin-400-0c94e034ca06357576c2d03d623e1fcd.woff2>; rel=preload; as=font; crossorigin=anonymous"
					},
					{
						"key": "Link",
						"value": "<https://www.google-analytics.com>; rel=preconnect;"
					}
				]
			},
			{
				"source": "sw.js",
				"headers": [
					{
						"key": "Cache-Control",
						"value": "public, max-age=0, must-revalidate"
					}
				]
			}
		]
	}
}
